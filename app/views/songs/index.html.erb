<ul>
<% @songs.each do |song| %>
  <li><%= link_to song.title, song_path(song) %></li>
  <% if song.artist %>
    <%= link_to display_artist(song), song.artist %>
  <% else %>
    <%= link_to "Add Artist", edit_song_path(song) %>
  <% end %>
<% end %>
</ul>
<br>
<%= link_to "Add New Song", new_song_path %>
