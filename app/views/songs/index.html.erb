<ul>
<% @songs.each do |song| %>
  <li><%= link_to song.title, song_path(song) %></li>

  <% if song.artist.nil? %>
    <h2><%= link_to "Add Artist", edit_song_path(song) %></h2> 
    <% else %>
    <li><%= display_artist(song) %></li>
  <% end %>
<% end %>
</ul>
<br>
<%= link_to "Add New Song", new_song_path %>
